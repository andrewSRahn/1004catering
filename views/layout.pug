	head
		title 1004Catering
		link(rel='stylesheet', href='/stylesheets/style.css')
		meta(name='viewport' content='width=device-width, initial-scale=1.0')
	body
		header
			div(id='language')
				span(class='en selected') english
				span  / 
				span(class='kr unselected') 한국어
			div(id='call')
				span Call Us Today! 562-407-1004 | lunch@1004catering.com
			div(id='logo')
				img(src='/images/LOGO.png' )
			nav
				div(class='korean' id='navigation-paragraphs')
					p 회사소개
					p 사진 겔러리
					p 결혼식
					p 돌/생신 잔치
					p 파티 및 행사
					p 런치
					p 궁금하세요?
				div(class='english' id='navigation-paragraphs')
					p Angel Catering
					p Gallery
					p Wedding
					p Birthday Party
					p Party & Event
					p Lunch

		block content

		footer
			div
				div(id='contact')
					h3 contact
					p 562-407-1004
					p 714-234-0864
					p 213-365-0006
					p Info@1004catering.com
					p 1004Catering
					p 14528 Carmenita Rd.
					p Norwalk, CA 90650
					a(href='https://www.facebook.com/pages/Angel-Catering/348531631977637')
						img(src='/images/facebook.ico' class='social-icon')
					a(href='https://www.yelp.com/biz/angel-catering-norwalk')
						img(src='/images/yelp.ico' class='social-icon')

				div
					hr(width='1' size='300')
				div
					h3 information
					p about us
					p contact us
				div
					h3 weddings
					p wedding menu
					p wedding event gallery
				div
					h3 birthday
					p birthday menu
					p birthday event
					p gallery
				div
					h3 events & party
					p event & party menu
					p event & party gallery
				div
					h3 lunch
					p lunch menu
					p lunch gallery
		script(src='https://code.jquery.com/jquery-3.3.1.min.js')
		script(src='/javascripts/index.js' type='text/javascript')




